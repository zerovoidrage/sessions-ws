# Database
DATABASE_URL="postgresql://user:password@localhost:5432/dbname?sslmode=require"

# LiveKit Configuration
# For LiveKit Cloud: use wss://your-project.livekit.cloud
# For self-hosted: use wss://your-livekit-hostname
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret
NEXT_PUBLIC_LIVEKIT_URL=wss://your-livekit-hostname

# OpenAI Realtime API
OPENAI_API_KEY=your_openai_api_key

# WebSocket Server (optional)
WS_PORT=3001
NEXT_PUBLIC_WS_PORT=3001
NEXT_PUBLIC_WS_HOST=localhost
